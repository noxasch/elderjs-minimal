{
  "name": "elderjs-app",
  "version": "1.0.20",
  "private": true,
  "main": "./src/build.js",
  "scripts": {
    "start": "npm run dev",
    "build": "node ./src/cleanPublic.js && npm run build:rollup && npm run build:html",
    "build:rollup": "rollup -c",
    "build:html": "node ./src/build.js",
    "dev": "rollup  -c -w --no-watch.clearScreen",
    "serve": "node ./src/cleanPublic.js && npm run build:rollup && NODE_ENV=production node ./src/server.js",
    "eslint": "eslint --ext .js .",
    "eslint:fix": "eslint --fix --ext .js .",
    "prettier:svelte": "prettier --write --plugin-search-dir=. ./**/*.svelte"
  },
  "type": "commonjs",
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@elderjs/elderjs": "^1.4.3",
    "@elderjs/plugin-browser-reload": "^0.1.37",
    "@elderjs/plugin-markdown": "^0.1.37",
    "@elderjs/plugin-seo-check": "0.1.37",
    "@elderjs/plugin-sitemap": "^0.1.39",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "intersection-observer": "^0.12.0",
    "node-fetch": "^2.6.1",
    "polka": "^0.5.2",
    "simple-svelte-autocomplete": "^1.3.2",
    "sirv": "^1.0.11",
    "svelte": "3.35.0",
    "svelte-preprocess": "^4.6.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.8",
    "prettier": "^2.3.0",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.41.1",
    "sass": "^1.34.0"
  },
  "author": "Nick Reese",
  "license": "MIT",
  "files": [
    "index.js",
    "elderjs"
  ],
  "keywords": [
    "svelte",
    "elderjs",
    "ssr",
    "static",
    "site",
    "generator",
    "seo",
    "ssg",
    "template",
    "partial",
    "hydration"
  ]
}
